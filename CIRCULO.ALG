Algoritmo "circulo"
//Digite o valor do raio do circulo: 2.0
//AREA = 12.566


Var
      raio,area : real

Inicio

      escreva("Digite o valor do raio do circulo: ")
      leia(raio)
      
      area <- Pi*(Exp(raio, 2))
      
      escreva("Área = ", area :8:3)



Fimalgoritmo